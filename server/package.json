  },
  "main": "main.js",
  "scripts": {
    "knex": "npx knex --knexfile database/knexfile.js",
    "start": "npx ts-node ./scripts/start_server.ts",
    "mirror": "npx ts-node ./scripts/import_mirror.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "halomaps",
    "forum",
    "archive",
    "halo",
