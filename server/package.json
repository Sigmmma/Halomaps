    "@types/node": "^20.2.5",
    "@types/polka": "^0.5.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "jsdom": "^21.1.0",
    "knex": "^2.4.1",
    "luxon": "^3.2.1",
    "minimist": "^1.2.7",
